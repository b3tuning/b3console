<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:controller="com.b3tuning.b3console.view.config.ConfigMenuView"
            AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0"
            xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">

    <AnchorPane styleClass="white-background-with-border" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0"
                AnchorPane.topAnchor="60.0">
        <Label styleClass="tile-text-sub" text="CONFIG" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="30.0"/>

        <AnchorPane fx:id="doorPlaceholder" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="130.0" AnchorPane.rightAnchor="30.0">
            <Label styleClass="tile-label-text" text="Door Module" AnchorPane.leftAnchor="0.0"
                   AnchorPane.topAnchor="0.0"/>
        </AnchorPane>
        <AnchorPane fx:id="shifterPlaceholder" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="130.0" AnchorPane.rightAnchor="30.0">
            <Label styleClass="tile-label-text" text="Shifter Module" AnchorPane.leftAnchor="0.0"
                   AnchorPane.topAnchor="0.0"/>
        </AnchorPane>
        <AnchorPane fx:id="transPlaceholder" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="130.0" AnchorPane.rightAnchor="30.0">
            <Label styleClass="tile-label-text" text="Trans Module" AnchorPane.leftAnchor="0.0"
                   AnchorPane.topAnchor="0.0"/>
        </AnchorPane>
    </AnchorPane>

    <Label styleClass="tile-text" text="CONFIGURATION" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="15.0"/>
</AnchorPane>
