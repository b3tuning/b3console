<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="pane" prefHeight="200.0" prefWidth="350.0" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0"
                AnchorPane.topAnchor="30.0">
        <Label fx:id="msgLabel" alignment="CENTER" prefHeight="40.0" prefWidth="280.0"
               styleClass="tile-label-text" wrapText="true" AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        <Button fx:id="okButton" mnemonicParsing="false" prefWidth="80.0" text="OK"
                AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0"/>
    </AnchorPane>
</AnchorPane>
