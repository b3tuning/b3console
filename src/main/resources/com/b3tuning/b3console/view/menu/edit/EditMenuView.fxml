<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<Menu xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.b3tuning.b3console.view.menu.edit.EditMenuView"
      text="Edit">
    <MenuItem fx:id="undoMenuItem" onAction="#undoAction" text="Undo"/>
    <MenuItem fx:id="redoMenuItem" onAction="#redoAction" text="Redo"/>
    <SeparatorMenuItem/>
    <MenuItem fx:id="cutMenuItem" onAction="#cutAction" text="Cut"/>
    <MenuItem fx:id="copyMenuItem" onAction="#copyAction" text="Copy"/>
    <MenuItem fx:id="pasteMenuItem" onAction="#pasteAction" text="Paste"/>
    <SeparatorMenuItem/>
    <MenuItem fx:id="deleteMenuItem" onAction="#deleteAction" text="Delete"/>
</Menu>
