<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<Menu xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.b3tuning.b3console.view.menu.edit.EditMenuView"
      fx:id="editMenu"
      text="Edit">
    <MenuItem onAction="#undoAction" text="Undo"/>
    <MenuItem onAction="#redoAction" text="Redo"/>
    <SeparatorMenuItem/>
    <MenuItem onAction="#cutAction" text="Cut"/>
    <MenuItem onAction="#copyAction" text="Copy"/>
    <MenuItem onAction="#pasteAction" text="Paste"/>
    <SeparatorMenuItem/>
    <MenuItem onAction="#deleteAction" text="Delete"/>
</Menu>
